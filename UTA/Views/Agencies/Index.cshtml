@model IEnumerable<UTA.Models.Agency>

@{
    ViewBag.Title = "Turističke Agencije";
}

<h2>Turističke Agencije</h2>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var agency in Model) {
    <tr>
      <td>@agency.Id</td>
      <td>@agency.Name</td>
    </tr>
    }
  </tbody>
</table>

